<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
        <bindings>
            <wsDualHttpBinding>
                <binding name="WSDualHttpBinding_IService" closeTimeout="00:00:10"
                    openTimeout="00:00:10" receiveTimeout="00:00:10" sendTimeout="00:00:10"
